<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <PackageId>Microsoft.Azure.WebJobs.Extensions.EventGrid</PackageId>
    <Description></Description>
    <Version>1.0.0-beta1$(VersionSuffix)</Version>
    <CommitHash Condition="$(CommitHash) == ''">N/A</CommitHash>
    <InformationalVersion>$(Version) Commit hash: $(CommitHash)</InformationalVersion>
  </PropertyGroup>

  <ItemGroup Condition="$(TargetFramework)=='net461'">
      <PackageReference Include="Microsoft.Azure.WebJobs" Version="2.1.0-beta1" />
      <PackageReference Include="Microsoft.Azure.WebJobs.Extensions" Version="2.1.0-beta1" />
      <Reference Include="System.Web" />
  </ItemGroup>

  <ItemGroup Condition="$(TargetFramework)=='netstandard2.0'">
      <PackageReference Include="Microsoft.Azure.WebJobs" Version="3.0.0-beta1-10941" />
      <PackageReference Include="Microsoft.Azure.WebJobs.Extensions" Version="3.0.0-beta1-10001" />
  </ItemGroup>
</Project>
